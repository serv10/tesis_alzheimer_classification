---
import ImageUploadingItem from "@components/ImageUploadingItem.astro";

const { selectedFile, uploadStatus, progress } = Astro.props;
---

<div class="w-full">
  <h5 class="text-gray-500 font-semibold">Uploading file</h5>
  <ImageUploadingItem
    selectedImage={selectedFile}
    uploadStatus={uploadStatus}
    progress={progress}
  />
</div>
