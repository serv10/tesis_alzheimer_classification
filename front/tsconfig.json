{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@icons/*": ["src/icons/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@modals/*": ["src/modals/*"],
      "@constants/*": ["src/constants/*"],
      "@dashboards/*": ["src/dashboards/*"]
    }
  }
}
